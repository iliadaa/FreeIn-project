{
  "name": "freein-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "start": "vue-cli-service serve --open  json-server --watch Users.json --port=8000",
    "servers": "concurrently \"json-server --watch Users.json --port=3000\" \"json-server --watch stages.json --port=3001\" \"json-server --watch itineraries.json --port=3002\" \"npm run serve\"",
    "deploy": "sh deploy.sh",
    "updater": "watch 'npm run build' src"
  },
  "dependencies": {
    "@fontsource/exo-2": "^4.5.8",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "bootstrap-vue": "^2.22.0",
    "core-js": "^3.8.3",
    "k-progress": "^1.5.0",
    "mdb-ui-kit": "^4.0.0",
    "mdb-vue-ui-kit": "^1.13.0",
    "pretty-checkbox-vue": "^1.1.9",
    "vue": "^2.6.14",
    "vue-prevent-unload": "^0.2.3",
    "vue-router": "^3.5.1",
    "vue-slick": "^1.1.20",
    "vue-slick-carousel": "^1.0.6",
    "vue-slide-bar": "^1.2.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-router": "^5.0.4",
    "@vue/cli-plugin-vuex": "^5.0.4",
    "@vue/cli-service": "^5.0.8",
    "concurrently": "^7.2.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-mdb5": "~1.2.0",
    "vue-template-compiler": "^2.6.10"
  },
  "browser": {
    "child_process": false
  }
}
